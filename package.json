{
    "name": "@gitbook-ng/gitbook-plugin-sitemap",
    "description": "Generate a sitemap for a GitBook",
    "main": "index.js",
    "version": "1.3.0",
    "engines": {
        "gitbook": ">=3.0.0"
    },
    "homepage": "https://github.com/gitbook-ng/plugin-sitemap",
    "keywords": ["seo"],
    "repository": {
        "type": "git",
        "url": "https://github.com/gitbook-ng/plugin-sitemap.git"
    },
    "dependencies": {
        "sitemap": "1.5.0"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/gitbook-ng/plugin-sitemap/issues"
    },
    "gitbook": {
        "properties": {
            "hostname": {
                "type": "string",
                "required": true,
                "title": "Hostname for the book"
            },
            "pathPrefix": {
                "type": "string",
                "required": true,
                "title": "Path prefix for sitemap link"
            }
        }
    }
}
